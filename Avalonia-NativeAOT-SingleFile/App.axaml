<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="Avalonia_NativeAOT_SingleFile.App"
             xmlns:local="using:Avalonia_NativeAOT_SingleFile"
             RequestedThemeVariant="Default">
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="/Styles/ThemeStyles.axaml" />
    </Application.Styles>
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <MergeResourceInclude Source="/Assets/ThemeDictionary.axaml" />
                <MergeResourceInclude Source="/Assets/FontIcons.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>
</Application>
